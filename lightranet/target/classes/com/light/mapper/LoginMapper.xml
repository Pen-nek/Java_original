<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.light.mapper.LoginMapper"> 
	<select id="login" resultType="com.light.domain.EmployeeVO"> 
		select * from employee 
		where e_loginid = #{e_loginid} and e_loginpw = #{e_loginpw}
	</select> 
</mapper>